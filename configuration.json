{ 
    "tenants": {
        "susi": {
            "vrf": "susi-1",
            "bd": "susi-1",
            "ap": "susi-1",
            "epg": ["susi-1", "susi-2"],
            "ctr": "allow-web",
            "status": "created"
        },

        "karhu": {
            "vrf": "karhu-1",
            "bd": "karhu-1",
            "ap": "karhu-1",
            "epg": ["karhu-1", "karhu-2"],
            "ctr": "allow-web",
            "status": "created"
        }
    },

    "filters": {
        "allow-ssh" : {
            "filt": "Allow-SSH",
            "entry": "SSH",
            "port_lower": "22",
            "port_upper": "22",
            "ethertype": "ipv4",
            "ip_protocol": "tcp"
        },

        "allow-https" : {
            "filt": "Allow-HTTPS",
            "entry": "HTTPS",
            "port_lower": "443",
            "port_upper": "443",
            "ethertype": "ipv4",
            "ip_protocol": "tcp"
        }
    },

    "contracts": {
        "allow-web" : {
            "con_name": "Allow-Web",
            "scope": "context",
            "sub_name": "Allow-Web",
            "filt": ["Allow-SSH", "Allow-HTTPS"]
        }
    }
}